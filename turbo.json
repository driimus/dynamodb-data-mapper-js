{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", ".docusarus/**"],
      "inputs": ["!*/**/*.spec.ts", "!*/**/*.integ.ts"],
      "outputLogs": "new-only"
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": [],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "format": {
      "outputs": []
    }
  }
}
