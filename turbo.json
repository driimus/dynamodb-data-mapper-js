{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", ".docusarus/**"],
      "inputs": ["!**/*.spec.ts", "!**/*.integ.ts", "!**/*jest.config.js"],
      "outputMode": "new-only"
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": [],
      "outputMode": "new-only"
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "format": {
      "outputs": []
    }
  }
}
