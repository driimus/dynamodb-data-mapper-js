{
  "name": "ddb-data-mapper",
  "version": "0.7.3",
  "description": "A schema-based data mapper for Amazon DynamoDB",
  "keywords": [
    "aws",
    "dynamodb"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awslabs/dynamodb-data-mapper-js.git"
  },
  "bugs": {
    "url": "https://github.com/awslabs/dynamodb-data-mapper-js/issues"
  },
  "homepage": "https://awslabs.github.io/dynamodb-data-mapper-js/packages/dynamodb-data-mapper/",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "docs": "typedoc src",
    "integ": "npm run pretest && jest --config=jest.integration.js",
    "prepublishOnly": "npm run build",
    "pretest": "tsc -p tsconfig.test.json",
    "test": "jest",
    "lint": "eslint --ext .ts .",
    "format": "eslint --fix --ext .ts ."
  },
  "author": {
    "name": "AWS SDK for JavaScript Team",
    "email": "aws-sdk-js@amazon.com"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.58.0",
    "@aws-sdk/types": "^3.55.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "aws-sdk-client-mock": "^0.6.2",
    "jest": "^27.5.1",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "ddb-auto-marshaller": "workspace:*",
